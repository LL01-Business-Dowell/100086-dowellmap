<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.dowell.dowellmap.fragment.SearchFragment"
        android:label="Home"
        tools:layout="@layout/fragment_search" >

        <action
            android:id="@+id/action_searchFragment_to_mapFragment"
            app:destination="@id/mapFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>




    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.dowell.dowellmap.fragment.MapFragment"
        android:label="DowellMap"
        tools:layout="@layout/fragment_map" >

        <argument
            android:name="placeModel"
            app:argType="com.dowell.dowellmap.data.model.PlaceDetail[]"
            app:nullable="true" />
    </fragment>
</navigation>